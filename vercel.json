{
  "version": 2,
  "name": "tasks-api",
  "buildCommand": "npm start",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}